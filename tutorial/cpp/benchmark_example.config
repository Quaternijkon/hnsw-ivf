# Faiss Benchmark配置文件示例
# 这个文件展示了如何配置不同的参数组合进行测试

build
  param
    # IVF索引的聚类中心数量，影响索引精度和内存使用
    nlist:1000,2000,4000,8000,16000
    # HNSW构建时的搜索参数，影响构建时间和索引质量
    efconstruction:32,64,128,256
  metric
    # 训练阶段内存使用量(MB)
    training_memory
    # 添加数据阶段内存使用量(MB)  
    add_memory
    # 训练时间(秒)
    training_time
    # 总构建时间(秒)
    total_time

search
  param
    # 搜索时访问的聚类中心数量，影响搜索精度和速度
    nprobe:8,16,32,64,128
    # HNSW搜索时的搜索参数，影响搜索精度和速度
    efsearch:8,16,32,64,128
  metric
    # 召回率，衡量搜索准确性
    recall
    # 每秒查询数，衡量搜索速度
    QPS
    # 搜索阶段内存使用量(MB)
    search_memory
    # 搜索时间(秒)
    search_time
    # 平均延迟(毫秒)
    mean_latency
    # 50分位延迟(毫秒)
    P50_latency
    # 99分位延迟(毫秒)
    P99_latency
